<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.17pretest" parent="Base.Theme.17pretest">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/lime_200</item>
        <item name="colorPrimaryVariant">@color/lime_200</item>
        <item name="colorPrimaryDark">@color/lime_600</item>
        <item name="colorOnPrimary">@color/black_600</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/gold_300</item>
        <item name="colorSecondaryVariant">@color/gold_300</item>
        <item name="colorOnSecondary">@color/black_600</item>
        <!-- Surface color. -->
        <item name="colorSurface">@color/black_500</item>
        <item name="colorOnSurface">@color/white_alpha_87</item>

        <item name="android:colorBackground">@color/black_600</item>
        <item name="colorOnBackground">@color/white_alpha_60</item>

        <item name="colorError">@color/pink_500</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSurface</item>

        <item name="android:windowLightStatusBar" tools:targetApi="m">false</item>
        <item name="android:windowLightNavigationBar" tools:ignore="NewApi">false</item>
    </style>
</resources>